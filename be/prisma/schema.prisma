generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  password  String
  role      Role     
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


enum Role {
  SUPER_ADMIN
  ADMIN
}